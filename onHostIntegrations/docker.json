      "props" : {
        "actions" : true,
        "query" : "SELECT average(docker.container.cpuPercent) FROM Metric TIMESERIES FACET `name`, `fullHostname`",
        "title" : "CPU Total (%)",
        "type" : "line",
        "endpoint" : "/v2/nrql",
        "event_types" : [ "ContainerSample" ]
      },
      "renderer" : "vizco"
