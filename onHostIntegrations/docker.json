{
  "template" : {
    "id" : "{{dashboard_id}}",
    "title" : "Docker",
    "filter" : {
      "key_names" : [ "containerId", "fullHostname", "hostname", "image", "imageName", "name" ],
      "event_types" : [ "ContainerSample" ],
      "filter_first" : false
    },
