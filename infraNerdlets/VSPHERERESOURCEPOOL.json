{
  "template" : {
    "title" : "VSphereResourcePoolSample",
    "filter" : { },
    "widgets" : [ {
      "name" : "Overall_status",
      "renderer" : "vizco",
      "layout" : {
        "height" : 3,
        "width" : 3,
        "x" : 0,
        "y" : 0
      },
      "props" : {
        "type" : "billboard",
        "title" : "Overall status",
        "hint" : "",
        "query" : "SELECT latest(`overallStatus`) FROM Metric WHERE `entityGuid` = '{{entity.id}}'"
      }
    }, {
      "name" : "Virtual_machines_count",
      "renderer" : "vizco",
      "layout" : {
        "height" : 3,
        "width" : 3,
        "x" : 3,
        "y" : 0
      },
      "props" : {
        "type" : "bar",
        "title" : "Virtual machines count",
        "hint" : "",
        "query" : "SELECT latest(`vsphere.resourcePool.vmCount`) FROM Metric WHERE `entityGuid` = '{{entity.id}}'"
      }
    }, {
      "name" : "CPU_overall_usage_and_total_MHz",
      "renderer" : "vizco",
      "layout" : {
        "height" : 3,
        "width" : 3,
        "x" : 6,
        "y" : 0
      },
      "props" : {
        "type" : "area",
        "title" : "CPU overall usage and total MHz",
        "hint" : "",
        "query" : "SELECT average(`vsphere.resourcePool.cpu.overallUsage`),average(`vsphere.resourcePool.cpu.totalMhz`) FROM Metric WHERE `entityGuid` = '{{entity.id}}' TIMESERIES auto"
      }
